{
    "name": "@alixlahuec/latex-roam",
    "version": "0.2.0",
    "description": "Exporter to LaTeX for Roam Research users",
    "license": "Apache-2.0",
    "author": {
        "name": "Alix Lahuec",
        "email": "alix.lahuec@onmail.com",
        "url": "https://ae-lhc.com"
    },
    "repository": "alixlahuec/latex-roam",
    "bugs": {
        "url": "https://github.com/alixlahuec/latex-roam/issues",
        "email": "alix.lahuec@onmail.com"
    },
    "main": ".src/index.js",
    "browser": "extension.js",
    "unpkg": "extension.js",
    "publishConfig": {
        "access": "public"
    },
    "scripts": {
        "build:roam": "webpack",
        "lint": "eslint",
        "ci:lint": "eslint --fix",
        "test": "jest",
        "ci:test": "jest --ci --collectCoverage",
        "storybook": "start-storybook -p 6006",
        "build-storybook": "build-storybook --quiet",
        "test-storybook": "test-storybook",
        "ci:test-storybook": "test-storybook --coverage",
        "release": "release-it minor --git.requireBranch=main",
        "release:beta": "release-it minor --git.requireBranch=beta major --preRelease=beta",
        "release:preview": "release-it --dry-run"
    },
    "devDependencies": {
        "@babel/core": "^7.17.9",
        "@babel/preset-env": "^7.16.11",
        "@babel/preset-react": "^7.18.6",
        "@storybook/addon-a11y": "^6.5.10",
        "@storybook/addon-actions": "^6.5.10",
        "@storybook/addon-controls": "^6.5.10",
        "@storybook/addon-coverage": "^0.0.3",
        "@storybook/addon-essentials": "^6.5.10",
        "@storybook/addon-interactions": "^6.5.10",
        "@storybook/addon-links": "^6.5.10",
        "@storybook/builder-webpack5": "^6.5.10",
        "@storybook/cli": "^6.5.10",
        "@storybook/jest": "^0.0.10",
        "@storybook/manager-webpack5": "^6.5.10",
        "@storybook/react": "^6.5.10",
        "@storybook/test-runner": "^0.5.0",
        "@storybook/testing-library": "^0.0.13",
        "@testing-library/react": "^12.1.5",
        "@testing-library/react-hooks": "^8.0.1",
        "axe-playwright": "^1.1.11",
        "babel-jest": "^28.1.3",
        "babel-loader": "^8.2.4",
        "css-loader": "^6.7.1",
        "css-minimizer-webpack-plugin": "^4.0.0",
        "eslint": "^8.13.0",
        "eslint-plugin-jest": "^26.8.0",
        "eslint-plugin-react": "^7.29.4",
        "eslint-plugin-react-hooks": "^4.4.0",
        "eslint-plugin-storybook": "^0.6.4",
        "jest": "^27.5.1",
        "jest-environment-jsdom": "^27.5.1",
        "jest-transform-css": "^4.0.1",
        "mini-css-extract-plugin": "^2.6.1",
        "release-it": "^15.3.0",
        "style-loader": "^3.3.1",
        "webpack": "^5.72.0",
        "webpack-cli": "^4.9.2",
        "webpack-merge": "^5.8.0"
    },
    "dependencies": {
        "@blueprintjs/core": "^4.1.2",
        "@blueprintjs/icons": "^4.2.3",
        "@blueprintjs/select": "^4.1.2",
        "jszip": "^3.10.0",
        "prop-types": "^15.8.1",
        "react": "^17.0.2",
        "react-dom": "^17.0.2"
    }
